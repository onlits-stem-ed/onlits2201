
	class Address {
		String street;
		String state;
		String pin;
		
		Address(String street, String state, String pin) {
		{
			this.street = street;
			this.state = state;
			//this.pin = pin;
		}
	}
	
	class Student {
		String name;
		Address address;	//composition
		
		Student() {
			name = "Mrityunjay Kumar";
			address = new Address("Bhagwanpur", "Bihar", "842001");
		}
		
		Student(String name, Address address) {
			this.name = name;
			this.address = address;
		}
	}
	
	Service Container
